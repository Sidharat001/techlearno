<!-- page-wrapper Start-->
<div class="page-wrapper compact-wrapper" id="pageWrapper">
    <div class="page-main-header">
        <div class="main-header-right row m-0">
            <div class="main-header-left">
                <div class="logo-wrapper">
                    <a href="/admin/dashboard"><img class="img-fluid" src="/admin/images/logo.png" alt=""></a>
                </div>
            </div>
            <div class="toggle-sidebar"><i class="status_toggle middle" data-feather="align-center"
                    id="sidebar-toggle"></i></div>
            <div class="left-menu-header col">
                <ul>
                    <li>
                        <form class="form-inline search-form">
                            <div class="search-bg"><i class="fa fa-search"></i></div>
                            <input class="form-control-plaintext" placeholder="Search here.....">
                        </form>
                    </li>

                </ul>

            </div> 
            <% if ( UserName && UserName.length> 0 ) { %>
                <div class="nav-right col pull-right right-menu">
                    <ul class="nav-menus">
                        <li class="onhover-dropdown">
                            <div class="media profile-media">
                                <img class="rounded-circle" src="/admin/images/logo.png" alt="">
                                <span><%= UserName %></span>
                            </div>
                            <ul class="profile-dropdown onhover-show-div">
                                <li><a href="/"><i data-feather="user"></i><span>Account </span></a></li>
                                <li><a href="/"><i data-feather="mail"></i><span>Inbox</span></a></li>
                                <li><a href="/"><i data-feather="file-text"></i><span>Taskboard</span></a></li>
                                <li><a href="/"><i data-feather="settings"></i><span>Settings</span></a></li>
                                <li>
                                    <form action="/admin/logout" method="get">
                                        <button type="submit"> <i data-feather="log-in"></i>Log out</button>
                                    </form>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
                <% } else { %>
                    <div class="nav-right col pull-right right-menu">
                        <ul class="nav-menus">
                            <li class="onhover-dropdown">
                                <div class="media profile-media"> <a href="/admin/loginuser"><i
                                            data-feather="user"></i><span>Login</span></a>
                                </div>
                            </li>
                        </ul>
                    </div>
                    <% } %>
        </div>
    </div>