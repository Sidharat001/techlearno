<%- include("../partial/header.ejs") %>
    <!-- Breadcrumb -->
    <section id="topbar">
        <div class="container">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item">
                        <a href="/">
                            <i class="fas fa-home"></i> Home
                        </a>
                    </li>
                    <li class="breadcrumb-item active"> Courses </li>
                </ol>
            </nav>
        </div>
    </section>

    <!-- Listings -->
    <section class="counts section-bg" id="listings" class="py-1">
        <div class="container">
            <div class="row"> 
                <!-- Listing 1 -->
                <% Data.map((item) => {  %>
                    <div class="col-md-6 col-lg-4 mb-4">
                        <div class="card listing-preview">
                            <img class="card-img-top" src="/media/download_19.png" alt="">
                            <div class="card-img-overlay">
                                <h2> <span class="badge badge-info"> <%= item.Price %></span> </h2>
                            </div>
                            <div class="card-body">
                                <div class="listing-heading text-center">
                                    <h6 style="text-align:center;" class="text-primary"><%= item.Title %><</h6>
                                    <p><i class="fas fa-map-marker text-secondary"></i>
                                        <h5><%= item.Title %></h5>
                                    </p>
                                </div>
                                <hr>
                            </div>
                        </div>
                        <a class="btn btn-info" href="/courses-details/<%- item.Seo_URL %>" role="button">More Information</a>
                    </div>
                <% }) %>
            </div>
        </div>
    </section>

    <%- include("../partial/footer.ejs") %>